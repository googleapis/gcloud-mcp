{
  "name": "@google-cloud/gcloud-mcp",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "workspaces": [
    "packages/*"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/googleapis/gcloud-mcp.git"
  },
  "bin": {
    "gcloud-mcp": "./packages/gcloud-mcp/dist/src/index.js"
  },
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "gts lint",
    "preflight": "npm ci && npm run format && npm run lint && npm run build && npm run test:ci",
    "test": "vitest run",
    "test:i": "vitest run --coverage",
    "clean": "gts clean",
    "compile": "tsc -p .",
    "fix": "gts fix",
    "prepare": "npm run compile",
    "pretest": "npm run compile",
    "posttest": "npm run lint"
  },
  "devDependencies": {
    "@tsconfig/strictest": "^2.0.5",
    "@types/node": "^24.2.1",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-license-header": "^0.8.0",
    "eslint-plugin-prettier": "^5.5.4",
    "gts": "^6.0.2",
    "turbo": "latest",
    "typescript": "^5.4.3",
    "vitest": "^3.2.4",
    "jsdom": "^24.1.1"
  },
  "packageManager": "npm@10.9.2"
}
